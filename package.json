{
  "name": "bumblebee-node-server",
  "description": "A backend server for an open-source testimonial side bar available for everyone to use and showcase the power of self-synchronizing products.",
  "version": "0.0.1",
  "main": "app.js",
  "repository": "git@github.com:workduck-io/bumblebee-node-server.git",
  "author": "Workduck <humans@workduck.io>",
  "license": "MIT",
  "engines": {
    "node": ">=14 <=16"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.ts,.tsx --fix",
    "start": "tsc && node dist/app.js",
    "build": "tsc",
    "dev": "nodemon src/app.ts",
    "debug": "export DEBUG=* && npm run start",
    "types": "typescript-json-schema \"src/interfaces/*.ts\" \"*\" -o src/types.json --id \"types\" --required"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.13",
    "@types/got": "^9.6.12",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "ajv": "7.2.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "got": "11.8.3",
    "html-entities": "^2.3.3",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "mem": "8.1.1",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "stats-map": "^1.0.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "typescript-json-schema": "^0.52.0",
    "winston": "^3.6.0"
  }
}
